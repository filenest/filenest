{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "isolatedModules": true,
        "allowJs": true,
        "declaration": true,
        "moduleResolution": "Node",
        "verbatimModuleSyntax": true,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "sourceMap": true
    }
}
