{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**",
                "build/**"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}